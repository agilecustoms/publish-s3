{
  "name": "publish-s3",
  "version": "2.1.6",
  "description": "Publish (upload) files from ./s3 directory to AWS S3",
  "keywords": [
    "aws",
    "github-action",
    "publish",
    "release",
    "s3",
    "upload"
  ],
  "homepage": "https://github.com/agilecustoms/publish-s3",
  "bugs": {
    "url": "https://github.com/agilecustoms/publish-s3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agilecustoms/publish-s3.git"
  },
  "license": "MIT",
  "author": "Alexey Chekulaev <chekulaevalexey@gmail.com> (https://github.com/laxa1986)",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=24.0.0"
  },
  "private": true,
  "scripts": {
    "prepare": "if [ \"$CI\" != \"true\" ]; then husky; fi",
    "lint": "eslint *.{ts,mjs} \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run --coverage",
    "build": "ncc build src/index.ts --minify; rm dist/package.json"
  },
  "dependencies": {
    "@actions/core": "^2.0.0",
    "@aws-sdk/client-s3": "^3.966.0",
    "@types/mime-types": "^2.1.4",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.7.0",
    "@testcontainers/localstack": "^11.8.0",
    "@types/node": "^24.10.6",
    "@vercel/ncc": "^0.38.3",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.9.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.7",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.2.0",
    "vitest": "^4.0.0"
  }
}
